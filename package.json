{
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "yarn markdown-toc -i",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "markdown-toc": "^1.2.0",
    "prettier": "^1.18.2"
  }
}
